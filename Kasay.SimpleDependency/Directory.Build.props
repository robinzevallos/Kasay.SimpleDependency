<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <Authors>Robin Zevallos</Authors>
    <Description>Simple dependency injection.</Description>
    <PackageTags>DependencyInjection, Dependency</PackageTags>
    <Version>1.0.5</Version>
  </PropertyGroup>

  <PropertyGroup>
    <PackageIconUrl>https://raw.githubusercontent.com/robinzevallos/Kasay.SimpleDependency/master/kasay_icon.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/robinzevallos/Kasay.SimpleDependency</PackageProjectUrl>
  </PropertyGroup>
  
  <PropertyGroup>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageOutputPath>$(SolutionDir)nugets</PackageOutputPath>
  </PropertyGroup>
</Project>